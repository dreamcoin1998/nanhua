.list-button{display:inline-block;width:100%;height:100%}.co-list{margin-top:40rpx;width:100%;background-color:#fff;border-radius:20.25rpx}.list{position:relative;width:90%;height:100rpx;margin:0 5%;border-bottom:1.5rpx solid #eee}.list-icon{position:absolute;left:0;z-index:1;display:inline-block;width:40rpx;height:40rpx;margin:30rpx 0}.list-text{z-index:1;display:inline-block;position:absolute;left:70rpx;font-size:32rpx;line-height:100rpx}.list-in{z-index:1;display:inline-block;position:absolute;right:30rpx;font-size:32rpx;line-height:100rpx;font-weight:400}.co-grid{margin-top:30rpx;border-radius:20.25rpx;width:100%;height:160rpx;background-color:#fff}.co-image{height:100rpx}.grid{width:calc(100% / 3);margin:10rpx auto;height:90%;display:inline-block;border-right-style:dashed;border-right-color:#bfbdb6;border-right-width:1rpx}.grid-click{width:calc(100% / 3);margin:10rpx auto;height:90%;display:inline-block;border-right-style:dashed;border-right-color:#bfbdb6;border-right-width:1rpx;background-color:#eee}.grid:nth-child(3n+3){border-right-color:#fff}.grid-text{display:block;text-align:center;line-height:10rpx;font-size:28rpx;font-weight:500}.grid-image{display:block;margin:15rpx auto;width:60rpx;height:60rpx}.blueBackground{height:450rpx;width:100%;background-color:#78acff}.avatar-button{height:190rpx;width:190rpx;border-radius:50%;-webkit-box-shadow:0rpx 0rpx 6rpx 8rpx #fff;box-shadow:0rpx 0rpx 6rpx 8rpx #fff}.avatar-image{width:190rpx;height:190rpx;position:absolute;z-index:1;border-radius:50%}.avatarPosition{position:absolute;top:50%;right:5%}.unAuth{line-height:190rpx;font-size:30rpx;margin:0 auto}.nickNamePosition{position:absolute;top:50%;left:5%}.big-white-bold{font-size:50rpx;font-weight:700;color:#fff}.statusPosition{position:absolute;bottom:25%;left:5%}.small-white-bold{font-size:24rpx;color:#fff}.main{margin:0 5% 0 5%}