<view><view class="lost_navigate"><text data-event-opts="{{[['tap',[['reportLost',['$event']]]]]}}" class="{{(('lost_text')+' '+(isActive?'active':''))}}" bindtap="__e">报失</text><text data-event-opts="{{[['tap',[['findLost',['$event']]]]]}}" class="{{(('lost_text')+' '+(isActive?'':'active'))}}" bindtap="__e">招领</text><button size="mini" data-event-opts="{{[['tap',[['lostPublish',['$event']]]]]}}" bindtap="__e">发布</button></view><swiper current="{{current}}" interval="{{2000}}" duration="{{1000}}"><swiper-item><view class="swiper-item"><block tt:for="{{[0,1,2,3,4]}}" tt:for-item="item" tt:for-index="__i0__"><lost-thing vue-id="{{'1-'+__i0__}}" lostName="{{lostName}}" lostOwner="{{lostOwner}}" lostTime="{{lostTime}}" bind:__l="__l"></lost-thing></block></view></swiper-item><swiper-item><view class="swiper-item"><block tt:for="{{[0,1,2,3,4,5,6]}}" tt:for-item="item" tt:for-index="__i1__"><lost-thing vue-id="{{'2-'+__i1__}}" lostName="{{lostName}}" lostOwner="{{lostOwner}}" lostTime="{{lostTime}}" bind:__l="__l"></lost-thing></block></view></swiper-item></swiper></view>