<view><view class="blueBackground" style="position:relative;"><view class="nickNamePosition big-white-bold">{{''+(userInfo?userInfo.nickName:"未授权")+''}}</view><block wx:if="{{userInfo}}"><view class="statusPosition small-white-bold">(身份未认证)</view></block><block wx:if="{{userInfo}}"><view class="avatarPosition"><image class="avatar-image" src="{{userInfo.avatarUrl}}" mode></image><button class="avatar-button" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['onGotUserInfo',['$event']]]]]}}" bindgetuserinfo="__e"></button></view></block><block wx:if="{{!userInfo}}"><button class="avatar-button avatarPosition unAuth" open-type="getUserInfo" data-event-opts="{{[['getuserinfo',[['onGotUserInfo',['$event']]]]]}}" bindgetuserinfo="__e">{{tip}}</button></block></view><view class="main"><view class="co-grid"><block wx:for="{{gridList}}" wx:for-item="items" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['touchstart',[['gridClick',['$0',index],[[['gridList','',index]]]]]],['touchend',[['gridEndClick',['$0',index],[[['gridList','',index]]]]]]]}}" class="{{[items.status?'grid-click':'grid']}}" bindtouchstart="__e" bindtouchend="__e"><view class="co-image"><image class="grid-image" src="{{items.src}}" mode></image></view><text class="grid-text">{{items.text}}</text></view></block></view><view class="co-list"><block wx:for="{{pageList}}" wx:for-item="items" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['listClick',['$0',index],[[['pageList','',index]]]]]]]}}" class="list" bindtap="__e"><button class="list-button" open-type="{{items.openType}}" hover-class="none"></button><image class="list-icon" src="{{items.src}}" mode></image><text class="list-text">{{items.text}}</text><text class="list-in">>></text></view></block></view></view></view>